public class Andor
{
    public static void main(String[] args) {
        int[] a = {1, 0, 1, 0, 1};
        int[] b = {0, 1, 1, 0, 0};

        for (int i = 0; i < 5; i++)
        {
            int result;
            if (a[i] == 1 && b[i] == 1)
            {
                result = 1;
            } else
            {
                result = 0;
            }
            System.out.printf("%d AND %d = %d%n", a[i], b[i], result);
        }
    }
}
